<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox id="VBox" fx:id="root" alignment="CENTER" prefHeight="366.0" prefWidth="563.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml" fx:controller="de.hs.settlers.gui.MapListViewController">
  <children>
    <TableView fx:id="mapTable" prefHeight="200.0" prefWidth="492.0" VBox.vgrow="ALWAYS">
      <columns>
        <TableColumn prefWidth="75.0" text="Name" fx:id="mapNameColumn" />
        <TableColumn prefWidth="75.0" text="Version" fx:id="mapVersionColumn" />
        <TableColumn prefWidth="75.0" text="Owner" fx:id="mapOwnerColumn" />
        <TableColumn prefWidth="75.0" text="Published" fx:id="mapPublishedColumn" />
      </columns>
      <contextMenu>
        <ContextMenu>
          <items>
            <MenuItem mnemonicParsing="false" text="Edit" />
            <MenuItem mnemonicParsing="false" text="Rename" />
            <MenuItem mnemonicParsing="false" text="Duplicate" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <MenuItem mnemonicParsing="false" text="Publish" />
          </items>
        </ContextMenu>
      </contextMenu>
    </TableView>
    <HBox id="HBox" spacing="5.0">
      <children>
        <Button mnemonicParsing="false" text="+" />
        <Button mnemonicParsing="false" text="-" />
        <Button mnemonicParsing="false" text="Modify" />
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
